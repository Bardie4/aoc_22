   "source": [
    "lines = open('mission.txt').read().splitlines()\n",
    "\n",
    "bag = []\n",
    "sum = 0\n",
    "for line in lines:\n",
    "    if line != '':\n",
    "        sum += int(line)\n",
    "    else:\n",
    "        bag.append(sum)\n",
    "        sum = 0\n",
    "\n",
    "print(max(bag))"
   ]
